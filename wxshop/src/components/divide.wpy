<style lang="less">
    .aui-flex-title{
        display: -webkit-box;
        display: -webkit-flex;
        display: flex;
        -webkit-box-align: center;
        -webkit-align-items: center;
        align-items: center;
        padding: 15px;
        position: relative;
        background: white;
        text-align: center;

        .aui-flex-box{
            -webkit-box-flex: 1;
            -webkit-flex: 1;
            flex: 1;
            min-width: 0;
            font-size: 14px;
            color: #333;
            
            .title{
                position: relative;
                width: 100%;
                font-size: 0.9rem;
                font-weight: normal;
            }
            .title:after {
                content: '';
                position: absolute;
                z-index: 2;
                bottom: 5px;
                left: 50%;
                margin-left: -50px;
                width: 15px;
                height: 15px;
                background-image: url("data:image/png;base64,");
                background-size: 15px;
            }
            .title:before {
                content: '';
                position: absolute;
                z-index: 2;
                bottom: 5px;
                right: 50%;
                margin-right: -50px;
                width: 15px;
                height: 15px;
                background-image: url("data:image/png;base64,");
                background-size: 15px;
            }
        }
    }
</style>

<template>
    <div class="aui-flex aui-flex-title" style="background:none">
        <div class="aui-flex-box">
            <div class="title">{{title}}</div>
        </div>
    </div>
</template>

<script>
    import wepy from '@wepy/core';

    wepy.component({
        props:{
            title:{
                type:String,
                default:''
            }
        }
        
    })
</script>
